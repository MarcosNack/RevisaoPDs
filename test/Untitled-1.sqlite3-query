-- database: ../db/db_itens_revision.db

-- Use the â–· button in the top right corner to run the entire file.

SELECT * FROM PD_CARAC_CONT WHERE PD = 'CONTATOR MANOBRA POTENCIA' AND CARACTERISTICA LIKE 'CONTATO AUXILIAR%';


DELETE FROM PD_CARAC_CONT WHERE PD = 'CONTATOR MANOBRA POTENCIA' AND CARACTERISTICA LIKE 'CONTATO AUXILIAR%' AND DESC_LONGA = '1NO+1NC';


UPDATE PD_CARAC_CONT
SET DESC_LONGA = 'TRIPOLAR'
WHERE PD = 'CONTATOR MANOBRA POTENCIA' AND CARACTERISTICA LIKE 'NUMERO POLOS%' AND CHAVE_CONT = '3';