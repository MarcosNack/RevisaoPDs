SELECT * FROM ITENS WHERE LONG_TEXT LIKE 'MOTOR ELE% ALT%' AND STATUS != "";

UPDATE ITENS SET STATUS = '' WHERE LONG_TEXT LIKE 'MOTOR ELE% ALT%' AND STATUS != "";

SELECT * FROM PD_CARAC_CONT WHERE PD LIKE 'PARAFUSO CABECA SEXTAVADA' AND CARACTERISTICA = "CARACTERISTICAS ADICIONAIS" AND DESC_CURTA = '';

UPDATE PD_CARAC_CONT SET DESC_CURTA = 'N/A' WHERE PD LIKE 'PARAFUSO CABECA SEXTAVADA' AND CARACTERISTICA = "CARACTERISTICAS ADICIONAIS" AND DESC_CURTA = '';